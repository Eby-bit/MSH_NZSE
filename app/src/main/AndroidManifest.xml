<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.sven.mysmarthome_beta">

    <application
            android:allowBackup="true"
            android:icon="@drawable/room"
            android:label="@string/app_name"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".garageActivity"
        />
        <activity android:name=".bathroomActivity"
        />
        <activity android:name=".kitchenActivity"
        />
        <activity android:name=".livingRoomActivity"
        />
    </application>

</manifest>
